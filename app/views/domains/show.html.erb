<h1><%= @domain.domain %></h1>
<p>
	<%= link_to 'Edit', edit_domain_path(@domain) %>
	<%= link_to 'Destroy', @domain, method: :delete, data: { confirm: 'Are you sure?' } %>
</p>

<p>
  <strong>Description:</strong>
  <%= @domain.description %>
</p>

<%= render 'shared/links', links: @domain.links %>
